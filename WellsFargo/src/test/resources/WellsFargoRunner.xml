<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="WellsFargo Automation Test or w.e id like" parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="os"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="62.0"/>
    <parameter name="url" value="https://www.wellsfargo.com/"/>


    <test name = "WellsFargo Tests">
        <classes>
            <class name="test.pages.HomePageTest"/>
        </classes>
    </test>

</suite>